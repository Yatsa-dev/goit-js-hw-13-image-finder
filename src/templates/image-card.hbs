<div class="photo-card">
    <a href ="{{largeImageURL}}"><img src="{{webformatURL}}" alt="{{tags}}" width="100vh" height="100vh" loading="lazy" /></a>
    <div class="info">
        <p class="info-item">
            <b>{{likes}}</b>
        </p>
        <p class="info-item">
            <b>{{views}}</b>
        </p>
        <p class="info-item">
            <b>{{comments}}</b>
        </p>
        <p class="info-item">
            <b>{{downloads}}</b>
        </p>
    </div>
</div>

